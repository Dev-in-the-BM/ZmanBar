<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema id="org.gnome.shell.extensions.zmanbar" path="/org/gnome/shell/extensions/zmanbar/">
    <key name="location-name" type="s">
      <default>''</default>
      <summary>Location Name</summary>
      <description>The name of the location for Zmanim, e.g., "New York, NY".</description>
    </key>
    <key name="latitude" type="d">
      <default>0.0</default>
      <summary>Latitude</summary>
      <description>The latitude of the location.</description>
    </key>
    <key name="longitude" type="d">
      <default>0.0</default>
      <summary>Longitude</summary>
      <description>The longitude of the location.</description>
    </key>
    <key name="enable-logging" type="b">
      <default>false</default>
      <summary>Enable Logging</summary>
      <description>Enable logging to the systemd journal.</description>
    </key>
    <key name="enable-file-logging" type="b">
      <default>false</default>
      <summary>Enable File Logging</summary>
      <description>Enable logging to a separate file in the extension's directory.</description>
    </key>
  </schema>
</schemalist>
